{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node builtScripts/startMsg.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node builtScripts/srcServer.js",
    "localtunnel": "lt --port 3100",
    "lint": "esw webpack.config.* src buildScript --color",
    "lint:watch": "npm run lint -- --watch",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress builtScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "braces": "^2.3.2",
    "nvm": "0.0.4",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^3.6.0",
    "eslint": "4.18.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-watch": "^7.0.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.16.1",
    "localtunnel": "^2.0.0",
    "mocha": "^8.0.1",
    "nock": "^13.0.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "^7.0.4",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.20.4",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
